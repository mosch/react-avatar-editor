    "watch": "rollup -c -w",
    "lint": "eslint --cache --ext .js,.jsx src",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "demo:build": "NODE_ENV=production webpack",
